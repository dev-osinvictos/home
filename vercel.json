{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "dashboard.html", "use": "@vercel/static" },
    { "src": "quotes.html", "use": "@vercel/static" },
    { "src": "store.html", "use": "@vercel/static" },
    { "src": "assets/**/*", "use": "@vercel/static" },
    { "src": "book/**/*", "use": "@vercel/static" },
    { "src": "resultados/**/*", "use": "@vercel/static" },
    { "src": "dashboard/**/*", "use": "@vercel/static" }
  ],

  "rewrites": [

    { "source": "/dashboard.html", "destination": "/dashboard.html" },
    { "source": "/dashboard", "destination": "/dashboard.html" },
    { "source": "/dashboard/", "destination": "/dashboard.html" },

    { "source": "/dashboard/acmilan", "destination": "/dashboard/acmilan/index.html" },
    { "source": "/dashboard/guaranifc", "destination": "/dashboard/guaranifc/index.html" },
    { "source": "/dashboard/cariverplate", "destination": "/dashboard/cariverplate/index.html" },
    { "source": "/dashboard/cabocajuniors", "destination": "/dashboard/cabocajuniors/index.html" },
    { "source": "/dashboard/crflamengo", "destination": "/dashboard/crflamengo/index.html" },
    { "source": "/dashboard/racingclub", "destination": "/dashboard/racingclub/index.html" },
    { "source": "/dashboard/scinternacional", "destination": "/dashboard/scinternacional/index.html" },
    { "source": "/dashboard/santosfc", "destination": "/dashboard/santosfc/index.html" },
    { "source": "/dashboard/juventusfc", "destination": "/dashboard/juventusfc/index.html" },
  { "source": "/dashboard/sepalmeiras", "destination": "/dashboard/sepalmeiras/index.html" },

  { "source": "/dashboard/(.*)", "destination": "/dashboard/$1" },

  { "source": "/assets/(.*)", "destination": "/assets/$1" },
  { "source": "/book/(.*)", "destination": "/book/$1" },
  { "source": "/resultados/(.*)", "destination": "/resultados/$1" },
  { "source": "/dashboard/(.*)", "destination": "/dashboard/$1" },

  { "source": "/(.*)", "destination": "/index.html" }
  ],

  "redirects": [
    {
      "source": "/resultados/FLAxRAC22102025",
      "destination": "/resultados/FLAxRAC22102025/index.html",
      "permanent": false
    },
    {
      "source": "/resultados/FLAxRAC22102025/redirect",
      "destination": "https://www.osinvictos.com.br/dashboard/crflamengo/index.html",
      "permanent": false
    }
  ],

  "headers": [
    {
      "source": "/resultados/FLAxRAC22102025/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Content-Type", "value": "text/html; charset=UTF-8" }
      ]
    },
    {
      "source": "/resultados/FLAxRAC22102025/bgFLAxRAC1a0.png",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Content-Type", "value": "image/png" }
      ]
    }
  ]
}

